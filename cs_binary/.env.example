# SafeNest native host environment configuration
# Copy this file to .env and fill values.

SAFETYPIN_SMTP_HOST=smtp.example.com
SAFETYPIN_SMTP_PORT=587
SAFETYPIN_SMTP_USER=your-user@example.com
SAFETYPIN_SMTP_PASS=your-smtp-password
SAFETYPIN_SMTP_SSL=true
SAFETYPIN_EMAIL_FROM=alerts@example.com

# Optional SMS (Twilio)
SAFETYPIN_TWILIO_ACCOUNT_SID=
SAFETYPIN_TWILIO_AUTH_TOKEN=
SAFETYPIN_TWILIO_FROM_PHONE=
